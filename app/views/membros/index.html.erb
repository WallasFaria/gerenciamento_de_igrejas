<div class="block">
  <div class="secondary-navigation">  
    <ul class="wat-cf">
      <li class="first"><%= link_to "Campo #{@filial.bairro}", filial_path(@filial.id) %></li>
      <li class="active"><%= link_to "Membros", filial_membros_path(@filial) %></li>
      <li><%= link_to "Novo", new_filial_membro_path(@filial.id) %></li>
    </ul>
  </div>          
  <div class="content">          
    <h2 class="title"><%= t("web-app-theme.all", :default => "All")  %> Membros</h2>
    <div class="inner">
      <table class="table">
        <tr>             
          <th class="first">ID</th>
          <th>
            <%= t("activerecord.attributes.membro.nome", :default => t("activerecord.labels.nome", :default => "Nome")) %>
          </th>
          <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @membros.each do |membro| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td> <%= membro.id %> </td>
          <td> <%= link_to membro.nome, filial_membro_path(@filial.id, membro) %> </td>
          <td> <%= membro.created_at.strftime('%d/%m/%y') %> </td>
          <td class="last">
            <%= link_to "Mostar", filial_membro_path(@filial.id, membro) %> |
            <%= link_to "Editar", edit_filial_membro_path(@filial.id, membro) %>
          </td>
        </tr>
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
        <%= will_paginate @membros  %>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
